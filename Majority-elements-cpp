
// major elements means An element is a major element if it appears more than n/2 times.
#include<iostream>
using namespace std;
int main(){
    int n=6;
    int arr[n]={1,1,2,2,2,3};
    for(int i=0;i<n;i++){
        int count = 0;
        for( int j=0;j<n;j++){           
            if(arr[i]==arr[j]){
                count++;
            }
            
        }
        if(count>=n/2){
            cout<<"major elements = "<<arr[i];
           break;
        }
    }
    return 0 ;
}
